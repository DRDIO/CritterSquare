<!DOCTYPE html>
<html>
    <head>
        <link rel="dns-prefetch" href="//ajax.googleapis.com" />
        <title>CritterSquare</title>
            
        <link rel="text/css" href="/css/design.min.css" />
    </head>

    <body>
        <header>
            <nav>
                <ul>
                    <li>
                        <a href="/home">Home</a>
                    </li>
                    <li>
                        <a href="/account">Account</a>
                    </li>
                </ul>
            </nav>
        </header>
        
        <section id="content">
            Loading Layout
        </section>

        <script>
            (function() {
                function getScript(url,success) {
                    var script = document.createElement('script');
                    script.src = url;
                    var head = document.getElementsByTagName('head')[0],
                        done = false;
        
                    script.onload=script.onreadystatechange = function() {
                        if (!done && (!this.readyState || this.readyState == 'loaded' || this.readyState == 'complete')) {
                            done = true;
                            success();
                            script.onload = script.onreadystatechange = null;
                            head.removeChild(script);
                        }
                    };
                
                    head.appendChild(script);
                }
        
                getScript('//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js', function() {
                    $.getScript('/js/src/dust-core-0.3.0.min.js', function() {
                        $.getScript('/js/layout.js');
                    });
                });
            })();
        </script>
    </body>
</html>